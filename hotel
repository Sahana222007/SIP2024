#include <stdio.h>

void main()
{
    int num;
    printf("Enter a number :");
    scanf("%d",&num);
    switch(num)
    {
        case 1:
        printf("pizza");
        break;
        case 2 :
        printf("Burger");
        break;
        case 3:
        printf("chicken noodles");
        break;
        default :
        printf("invalid code");
        break;
    }

    
}
